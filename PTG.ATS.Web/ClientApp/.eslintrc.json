{
	"extends": "react-app",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"jquery": true,
		"es6": true
	},
	"rules": {
		"block-scoped-var": 2,
		"camelcase": 2,
		"complexity": [0, 11],
		"curly": 2,
		"dot-location": 0,
		"dot-notation": [2, { "allowKeywords": true}],
		"eqeqeq": 2,
		"guard-for-in": 1,
		"jsx-quotes": 1,
		"new-cap": 0,
		"new-parens": 1,
		"no-alert": 0,
		"no-else-return": 2,
		"no-extra-bind": 2,
		"no-bitwise": 2,
		"no-caller": 2,
		"no-eval": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-magic-numbers": 0,
		"no-mixed-spaces-and-tabs": 1,
		"no-multi-spaces": 0,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-redeclare": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-trailing-spaces": 1,
		"no-undef": 2,
		"no-unused-vars": ["error", { "args": "none" }],
		"no-use-before-define": 2,
		"no-with": 2,
		"one-var":[1,"never"],
		"one-var-declaration-per-line":1,
		"vars-on-top": 2,
		"wrap-iife": 2,
		"yoda": 2
	},
	"globals": {
		"module": true,
		"process": true,
		"require": true,
		"Modernizr": true,
		"jQuery": true,
		"$": true,
		"_": true,
		"ga": true,
		"YT": true,
		"Backbone": true,
		"Handlebars": true,
		"HandlebarsHelpers": true,
		"ScrollMagic": true,
		"TweenMax": true,
		"TimelineMax": true,
		"Draggable": true,
		"Linear": true,
		"Back": true,
		"Bounce": true,
		"Circ": true,
		"Cubic": true,
		"Elastic": true,
		"Expo": true,
		"Quad": true,
		"Quart": true,
		"Quint": true,
		"Sine": true,
		"Strong": true,
		"sendTrackEvent": true,
		"addeventasync": true,
		"addeventatc": true,
		"Application": true
	}
}
